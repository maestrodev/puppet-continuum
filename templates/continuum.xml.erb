<?xml version="1.0" encoding="UTF-8"?>
<continuum-configuration>
  <baseUrl><%= application_url_real %></baseUrl>
  <buildOutputDirectory><%= installbase %>/data/build-output-directory</buildOutputDirectory>
  <workingDirectory><%= installbase %>/data/working-directory</workingDirectory>
  <releaseOutputDirectory><%= installbase %>/data/release-output-directory</releaseOutputDirectory>
<% if @shared_secret_password -%>
  <distributedBuildEnabled>true</distributedBuildEnabled>
  <sharedSecretPassword><%=shared_secret_password%></sharedSecretPassword>
<% end -%>
</continuum-configuration>
